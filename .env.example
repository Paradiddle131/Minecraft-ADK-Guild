# Minecraft Multi-Agent Configuration
# Copy this file to .env and set your values

# Google AI API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
MINECRAFT_AGENT_GOOGLE_AI_API_KEY=your_gemini_api_key_here

# Alternative: Google Cloud Vertex AI Configuration
# MINECRAFT_AGENT_GOOGLE_CLOUD_PROJECT=your-project-id
# MINECRAFT_AGENT_GOOGLE_CLOUD_LOCATION=us-central1

# Agent Configuration
MINECRAFT_AGENT_DEFAULT_MODEL=gemini-2.0-flash
MINECRAFT_AGENT_AGENT_TEMPERATURE=0.2
MINECRAFT_AGENT_MAX_OUTPUT_TOKENS=500

# Minecraft Server Configuration
MINECRAFT_AGENT_MINECRAFT_HOST=localhost
MINECRAFT_AGENT_MINECRAFT_PORT=25565
MINECRAFT_AGENT_BOT_USERNAME=MinecraftAgent

# Bridge Configuration
MINECRAFT_AGENT_COMMAND_TIMEOUT_MS=10000
MINECRAFT_AGENT_EVENT_QUEUE_SIZE=1000